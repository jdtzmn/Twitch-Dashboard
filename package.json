{
  "name": "twitch-dashboard",
  "productName": "Twitch Dashboard",
  "version": "1.0.0",
  "description": "A twitch dashboard for clutter free twitch streaming",
  "main": "main.js",
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-mac": "./node_modules/.bin/electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/icon@2x.icns --prune=true --out=release-builds",
    "package-win": "./node_modules/.bin/electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/icon@2x.ico --prune=true --out=release-builds --version-string.CompanyName=jdtzmn --version-string.FileDescription=\"Twitch Dashboard\" --version-string.ProductName=\"Twitch Dashboard\""
  },
  "author": "jdtzmn <jdtzmn@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.7.3",
    "electron-packager": "^8.7.1"
  },
  "dependencies": {
    "about-window": "^1.6.3",
    "jquery": "^3.2.1"
  }
}
